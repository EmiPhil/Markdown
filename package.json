{
  "name": "Markdown_CodeHorror",
  "version": "0.0.1",
  "description": "A tutorial for the popular Markdown language.",
  "author": "EmiPhil",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmiPhil/Markdown"
  },
  "bugs": {
    "url": "https://github.com/EmiPhil/Markdown/issues"
  },
  "keywords": [
    "markdown",
    "code",
    "horror",
    "tutorial"
  ],
  "scripts": {
    "start": "gulp"
  },
  "devDependencies": {
    "gulp": "3.8.11",
    "gulp-run": "1.6.5",
    "gulp-sass": "~1.3.3",
    "gulp-concat": "2.4.3",
    "gulp-uglify": "1.0.2",
    "gulp-minify-css": "0.3.11",
    "gulp-jshint": "1.9.0",
    "browser-sync": "1.8.2",
    "del": "1.1.1",
    "jshint-stylish": "~1.0.1"
  },
  "paths": {
    "sass": "./source/**/*.scss",
    "js": "./source/**/*.js",
    "html": "./public/*.html"
  },
  "dest": {
    "style": "style.css",
    "app": "bundle.js",
    "distcss": "public/assets/css",
    "distjs": "public/assets/js"
  }
}
